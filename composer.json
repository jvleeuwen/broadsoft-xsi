{
    "name": "Jvleeuwen/BroadsoftXSI",
    "license": "MIT",
    "authors": [
        {
            "name": "Jelle van Leeuwen",
            "email": "info@jellevl.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "pusher/pusher-php-server": "^2.6",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "laravel/laravel": "dev-develop",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "^3.5",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Jvleeuwen\\BroadSoftXSI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jvleeuwen\\BroadsoftXSI\\Test\\" : "TestCase.php"
        }
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "Jvleeuwen\\BroadsoftXSI\\BroadsoftXSIServiceProvider::class",
                "App\\Providers\\BroadcastServiceProvider::class"
            ]
        }
    }
}
